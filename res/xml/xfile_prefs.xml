    </PreferenceScreen>
    <PreferenceScreen android:title="@string/xfile_buffer_title" android:key="buffer_screen">
        <EditTextPreference android:numeric="integer" android:title="@string/xfile_maximum_buffer_title" android:key="pref_max_buffer_ms" android:summary="@string/xfile_maximum_buffer_summary" android:defaultValue="120000" />
