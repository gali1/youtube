        <EditTextPreference android:numeric="integer" android:title="@string/xfile_rebuffer_title" android:key="pref_buffer_for_playback_after_rebuffer_ms" android:summary="@string/xfile_rebuffer_summary" android:defaultValue="5000" />
    </PreferenceScreen>
