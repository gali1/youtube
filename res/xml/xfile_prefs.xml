    <PreferenceScreen android:title="@string/xfile_misc_title" android:key="misc_screen">
        <SwitchPreference android:title="@string/xfile_auto_repeat_button_title" android:key="pref_auto_repeat_button" android:defaultValue="false" android:summaryOn="@string/xfile_auto_repeat_button_summary_on" android:summaryOff="@string/xfile_auto_repeat_button_summary_off" />
        <SwitchPreference android:title="@string/xfile_auto_repeat_title" android:key="pref_auto_repeat" android:defaultValue="false" android:summaryOn="@string/xfile_auto_repeat_summary_on" android:summaryOff="@string/xfile_auto_repeat_summary_off" />
        <ListPreference android:entries="@array/vanced_button_location_entries" android:title="@string/xfile_copy_video_url_timestamp_title" android:key="pref_copy_video_url_timestamp_button_list" android:entryValues="@array/vanced_button_location_entry_values" />
        <ListPreference android:entries="@array/vanced_button_location_entries" android:title="@string/xfile_copy_video_url_title" android:key="pref_copy_video_url_button_list" android:entryValues="@array/vanced_button_location_entry_values" />
        <SwitchPreference android:title="@string/xfile_hdr_full_brightness_title" android:key="pref_hdr_autobrightness" android:defaultValue="true" android:summaryOn="@string/xfile_hdr_full_brightness_summary_on" android:summaryOff="@string/xfile_hdr_full_brightness_summary_off" />
