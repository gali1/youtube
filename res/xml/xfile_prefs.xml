        <SwitchPreference android:title="@string/xfile_xfenster_brightness_title" android:key="pref_xfenster_brightness" android:defaultValue="false" android:summaryOn="@string/xfile_xfenster_brightness_summary_on" android:summaryOff="@string/xfile_xfenster_brightness_summary_off" />
        <SwitchPreference android:title="@string/xfile_xfenster_volume_title" android:key="pref_xfenster_volume" android:defaultValue="false" android:summaryOn="@string/xfile_xfenster_volume_summary_on" android:summaryOff="@string/xfile_xfenster_volume_summary_off" />
        <SwitchPreference android:title="@string/xfile_xfenster_tablet_title" android:key="pref_xfenster_tablet" android:defaultValue="false" android:summaryOn="@string/xfile_xfenster_tablet_summary_on" android:summaryOff="@string/xfile_xfenster_tablet_summary_off" />
        <EditTextPreference android:numeric="integer" android:title="@string/xfile_swipe_threshold_title" android:key="pref_xfenster_swipe_threshold" android:summary="@string/xfile_swipe_threshold_summary" android:defaultValue="0" />
