<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:yt="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory android:title="@string/microg_settings">
        <PreferenceScreen android:title="@string/microg_notification_settings" android:summary="@string/microg_notification_settings_summary">
            <intent android:targetPackage="com.mgoogle.android.gms" android:targetClass="org.microg.gms.ui.SettingsActivity" />
        </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/vanced_settings" />
    <PreferenceScreen android:title="@string/xfile_codec_override_title" android:key="codec_override">
        <SwitchPreference android:title="@string/xfile_vp9_title" android:key="vp9_xfile_enabled" android:defaultValue="false" android:summaryOn="@string/xfile_vp9_summary_on" android:summaryOff="@string/xfile_vp9_summary_off" />
        <Preference android:persistent="false" android:title="@string/xfile_default_codec_title" android:selectable="true" android:key="pref_default_override" android:summary="@string/xfile_default_codec_summary" />
        <Preference android:persistent="false" android:title="@string/xfile_vp9_title" android:selectable="true" android:key="pref_vp9_override" android:summary="@string/xfile_vp9_summary" />
        <Preference android:persistent="false" android:title="@string/xfile_hardware_hdr_title" android:selectable="true" android:key="pref_hdrhardware_override" android:summary="@string/xfile_hardware_hdr_summary" />
        <Preference android:persistent="false" android:title="@string/xfile_software_hdr_title" android:selectable="true" android:key="pref_hdrsoftware_override" android:summary="@string/xfile_software_hdr_summary" />
        <EditTextPreference android:singleLine="true" android:title="@string/xfile_current_override_manufacturer" android:key="override_manufacturer" />
        <EditTextPreference android:singleLine="true" android:title="@string/xfile_current_override_model" android:key="override_model" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/xfile_video_settings_title" android:key="video_settings">
        <SwitchPreference android:title="@string/xfile_old_style_quality_settings_title" android:key="old_style_quality_settings" android:defaultValue="true" android:summaryOn="@string/xfile_old_style_quality_settings_summary_on" android:summaryOff="@string/xfile_old_style_quality_settings_summary_off" />
        <SwitchPreference android:title="@string/xfile_override_resolution_title" android:key="override_resolution_xfile_enabled" android:defaultValue="false" android:summaryOn="@string/xfile_override_resolution_summary_on" android:summaryOff="@string/xfile_override_resolution_summary_off" />
        <ListPreference android:title="@string/xfile_preferred_video_quality_wifi_title" android:key="pref_preferred_video_quality_wifi" android:summary="@string/xfile_preferred_video_quality_wifi_summary" />
        <ListPreference android:title="@string/xfile_preferred_video_quality_mobile_title" android:key="pref_preferred_video_quality_mobile" android:summary="@string/xfile_preferred_video_quality_mobile_summary" />
        <ListPreference android:title="@string/xfile_preferred_video_speed_title" android:key="pref_preferred_video_speed" android:summary="@string/xfile_preferred_video_speed_summary" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/vanced_video_ad_settings_title" android:key="video_ad_settings">
