        <SwitchPreference android:title="@string/xfile_branding_watermark_title" android:key="branding_watermark_enabled" android:defaultValue="true" android:summaryOn="@string/xfile_branding_watermark_summary_on" android:summaryOff="@string/xfile_branding_watermark_summary_off" />
        <SwitchPreference android:title="@string/xfile_cast_button_title" android:key="cast_button_enabled" android:defaultValue="true" android:summaryOn="@string/xfile_cast_button_summary_on" android:summaryOff="@string/xfile_cast_button_summary_off" />
        <SwitchPreference android:title="@string/xfile_tablet_miniplayer_title" android:key="tablet_miniplayer" android:defaultValue="false" android:summaryOn="@string/xfile_tablet_miniplayer_summary_on" android:summaryOff="@string/xfile_tablet_miniplayer_summary_off" />
        <SwitchPreference android:title="@string/xfile_create_button_title" android:key="xfile_create_button_hidden" android:defaultValue="false" android:summaryOn="@string/xfile_create_button_summary_on" android:summaryOff="@string/xfile_create_button_summary_off" />
